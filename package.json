{
  "name": "@salesforce/plugin-dev",
  "description": "commands for sf plugin development",
  "version": "0.7.0",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^2.0.7",
    "@octokit/rest": "^19.0.5",
    "@salesforce/core": "^3.32.12",
    "@salesforce/kit": "^1.8.2",
    "@salesforce/sf-plugins-core": "^2.2.4",
    "change-case": "^4.1.2",
    "fast-glob": "^3.2.12",
    "got": "^11.8.5",
    "graphology": "^0.25.1",
    "graphology-types": "^0.24.7",
    "js-yaml": "^4.1.0",
    "replace-in-file": "^6.3.2",
    "shelljs": "^0.8.5",
    "tslib": "^2",
    "yeoman-environment": "^3.13.0",
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.3.5",
    "@oclif/test": "^2.3.4",
    "@salesforce/cli-plugins-testkit": "^3.2.17",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.1",
    "@salesforce/plugin-command-reference": "^2.2.10",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.4.4",
    "@swc/core": "^1.3.29",
    "@types/js-yaml": "^4.0.5",
    "@types/shelljs": "^0.8.10",
    "@types/yeoman-generator": "^5.2.10",
    "@types/yeoman-test": "^4.0.3",
    "@types/yosay": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.52.0",
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-sf-plugin": "^1.8.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.6.1",
    "prettier": "^2.8.2",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.3",
    "sinon": "10.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.4",
    "yeoman-test": "^6.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas",
    "/templates"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-dev",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin",
    "sf-plugin",
    "sf"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "dev": {
        "description": "Commands for sf plugin development.",
        "subtopics": {
          "convert": {
            "description": "description for dev.convert"
          },
          "generate": {
            "description": "Commands for generating sf plugins, commands, and hooks."
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-dev",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy",
    "prepack": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test --timeout 600000",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:nuts": "nyc mocha \"test/**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  }
}
