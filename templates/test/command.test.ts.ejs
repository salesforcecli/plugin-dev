import { expect, test } from '@oclif/test';

describe('<%- name %>', () => {
  test
    .stdout()
    .command(['<%- name %>'])
    .it('runs hello', (ctx) => {
      expect(ctx.stdout).to.contain('hello world');
    });

  test
    .stdout()
    .command(['<%- name %>', '--name', 'Astro'])
    .it('runs hello --name Astro', (ctx) => {
      expect(ctx.stdout).to.contain('hello Astro');
    });
});
